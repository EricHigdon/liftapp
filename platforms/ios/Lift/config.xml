<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="3" id="com.accordapp.lift" version="0.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="RemoteControls">
        <param name="ios-package" value="RemoteControls" />
    </feature>
    <feature name="RareloopAppVersion">
        <param name="ios-package" value="RareloopAppVersion" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <feature name="MusicControls">
        <param name="ios-package" value="MusicControls" />
    </feature>
    <feature name="PushNotification">
        <param name="ios-package" value="PushPlugin" />
    </feature>
    <name>Lift</name>
    <description>Lift For Life Camps</description>
    <author email="eric.s.higdon@gmail.com" href="http://accordapp.com">Eric Higdon</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <splash src="splash.png" />
    <access origin="*" />
    <allow-navigation href="https://www.youtube.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="cdvfile://*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <config-file mode="replace" parent="UIBackgroundModes" platform="ios">
        <array>
            <string>remote-notification</string>
        </array>
    </config-file>
    <icon height="60" src="www/res/icons/ios/icon-60.png" width="60" />
    <icon height="120" src="www/res/icons/ios/icon-60-2x.png" width="120" />
